{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/planarity", "dest": "api/index.py" },

    { "src": "/", "dest": "/index.html" },
    { "src": "/script.js", "dest": "/script.js" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },

    { "src": "/(.*)", "dest": "/$1" }
  ]
}
